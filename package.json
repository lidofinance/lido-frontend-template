{
  "name": "lido-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_OPTIONS='-r next-logger' node next-start-cluster.js",
    "start:legacy": "NODE_OPTIONS='-r next-logger' next start",
    "lint": "eslint --ext ts,tsx .",
    "lint:fix": "yarn lint --fix",
    "typechain": "typechain --target=ethers-v5 --out-dir ./generated ./abi/*.json",
    "postinstall": "husky install && yarn typechain",
    "commit": "git-cz"
  },
  "dependencies": {
    "@darkobits/mask-string": "^2.0.1",
    "@ethersproject/abstract-signer": "^5.5.0",
    "@ethersproject/address": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/constants": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@ethersproject/units": "^5.6.1",
    "@lido-sdk/constants": "^1.6.0",
    "@lido-sdk/contracts": "^2.0.0",
    "@lido-sdk/fetch": "^2.1.0",
    "@lido-sdk/helpers": "^1.4.3",
    "@lido-sdk/react": "^1.18.1",
    "@lido-sdk/web3-react": "^1.27.2",
    "@lidofinance/lido-ui": "^3.0.0-next.17",
    "@lidofinance/lido-ui-blocks": "^2.10.2",
    "@lidofinance/next-api-wrapper": "^0.6.0",
    "@lidofinance/next-cache-files-middleware": "^0.3.0",
    "cookie": "^0.4.2",
    "copy-to-clipboard": "^3.3.1",
    "ethers": "^5.5.3",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "js-cookie": "^3.0.1",
    "memory-cache": "^0.2.0",
    "next": "^12.2.5",
    "next-logger": "^3.0.0",
    "next-secure-headers": "^2.2.0",
    "prom-client": "^14.0.1",
    "react": "17.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "remark": "^13.0.0",
    "remark-external-links": "^8.0.0",
    "remark-html": "^13.0.1",
    "styled-components": "^5.3.5",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@commitlint/prompt": "^15.0.0",
    "@svgr/webpack": "^5.5.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/cookie": "^0.4.1",
    "@types/js-cookie": "^3.0.1",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^18.6.1",
    "@types/react": "^17.0.15",
    "@types/styled-components": "^5.1.23",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "commitizen": "^4.2.4",
    "eslint": "8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "typechain": "^5.1.2",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "eslint --ignore-path .gitignore --max-warnings=0"
    ],
    "./**/*.{ts,tsx,css,md,json}": [
      "prettier --write"
    ]
  },
  "commintlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  }
}
